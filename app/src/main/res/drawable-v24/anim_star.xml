<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group android:name="group">
                <group android:name="filled_star">
                    <clip-path
                        android:name="star"
                        android:pathData="M 20.8 10 C 20.6 9.5 14.8 9.1 14.7 9.1 C 14.7 9.1 12.5 3.6 12 3.6 C 11.5 3.6 9.3 9.1 9.3 9.1 C 9.3 9.1 3.4 9.4 3.2 10 C 3 10.5 7.6 14.3 7.6 14.3 C 7.6 14.3 6.1 20 6.6 20.4 C 7.1 20.8 12 17.6 12 17.5 C 12 17.5 17 20.7 17.4 20.4 C 17.8 20.1 16.4 14.4 16.4 14.3 C 16.4 14.3 21 10.5 20.8 10 Z"/>
                    <path
                        android:name="fill"
                        android:pathData="M 3.5 21.9 L 20.5 21.9 L 21.4 22.5 L 3.2 22.5 Z"
                        android:fillColor="#000"
                        android:strokeWidth="1"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="fill">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="674"
                android:valueFrom="M 10.48 22.5 C 9.267 22.5 8.053 22.5 6.84 22.5 C 5.627 22.5 4.413 22.5 3.2 22.5 L 3.5 21.9 C 9.167 21.9 14.833 21.9 20.5 21.9 C 20.8 22.1 21.1 22.3 21.4 22.5 C 20.187 22.5 18.973 22.5 17.76 22.5 L 14.12 22.5 L 10.48 22.5"
                android:valueTo="M 3.2 20.9 C 3.2 20.9 2.7 18.9 2 14.8 C 1.3 10.7 3.2 2.9 3.2 2.9 L 3.2 2.9 C 3.2 2.9 4.3 0.1 7.7 0.5 C 11.1 0.9 20.5 0.3 21.5 2.9 C 22.5 5.5 20.7 15 20.7 15 L 21.4 20.9 L 3.2 20.9"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="filled_star">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="358"
                    android:duration="259"
                    android:valueFrom="0"
                    android:valueTo="2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="696"
                    android:valueFrom="15"
                    android:valueTo="-6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:startOffset="696"
                android:duration="206"
                android:valueFrom="0"
                android:valueTo="6"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
